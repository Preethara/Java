public class Main {
 public static int search(int[] arr, int key){
    for(int i=1;i<arr.length;i++)
    {
        if(arr[i] == key)
        return arr[i];
    }
        return -1;   
 }
	public static void main(String[] args) {
		int[] arr = {10,5,67,89,505};
		int key = 9;
		int result = search(arr, key);
		System.out.print(result == -1 ? "Not found":"Found at"+result);
	}
}